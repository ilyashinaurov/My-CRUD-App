<Page x:Class="КУРСОВАЯ.Views.Car.carListPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:КУРСОВАЯ.Views.Car"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="carListPage">

    <Grid>
        <ListView ItemsSource="{Binding _carList}">
            <ListView.View>
                <GridView>
                    <GridViewColumn Header="Код ТС" DisplayMemberBinding="{Binding КодТС}"/>
                    <GridViewColumn Header="Госномер" DisplayMemberBinding="{Binding Госномер}"/>
                    <GridViewColumn Header="Код Прицепа" DisplayMemberBinding="{Binding КодПрицепа}"/>
                    <GridViewColumn Header="Код Модели" DisplayMemberBinding="{Binding КодМоделиТС}"/>

                </GridView>
            </ListView.View>
        </ListView>
    </Grid>
</Page>
